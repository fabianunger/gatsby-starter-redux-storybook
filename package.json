{
  "name": "gatsby-starter-redux-storybook",
  "description": "Gatsby Redux Storybook Starter",
  "version": "1.0.0",
  "author": "Fabian Unger <hello@fabianunger.de>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fabianunger/gatsby-starter-redux-storybook"
  },
  "keywords": [
    "gatsby",
    "redux",
    "storybook",
    "eslint",
    "styled-components",
    "reset-css",
    "nelify"
  ],
  "scripts": {
    "start": "run-p develop:**",
    "develop": "run-p develop:**",
    "develop:gatsby": "gatsby develop -H 0.0.0.0 -o",
    "develop:storybook": "start-storybook -p 9000 -c .storybook",
    "storybook": "start-storybook -p 9000 -c .storybook",
    "build": "run-p 'build:!(storybook)'",
    "build:gatsby": "gatsby build",
    "build:storybook": "build-storybook -c .storybook -o storybook",
    "serve": "gatsby serve -o -p 8080",
    "clean": "gatsby clean"
  },
  "dependencies": {
    "@babel/core": "^7.8.4",
    "babel-loader": "^8.0.6",
    "gatsby": "^2.15.6",
    "gatsby-image": "^2.2.17",
    "gatsby-plugin-manifest": "^2.2.14",
    "gatsby-plugin-offline": "^3.0.2",
    "gatsby-plugin-react-helmet": "^3.1.6",
    "gatsby-plugin-root-import": "^2.0.5",
    "gatsby-plugin-sharp": "^2.2.19",
    "gatsby-plugin-styled-components": "^3.1.4",
    "gatsby-source-filesystem": "^2.1.21",
    "gatsby-transformer-sharp": "^2.2.13",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "styled-components": "^4.3.2",
    "styled-reset-advanced": "^1.0.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.13",
    "@storybook/addon-links": "^5.3.13",
    "@storybook/addon-notes": "^5.3.13",
    "@storybook/addons": "^5.3.13",
    "@storybook/react": "^5.3.13",
    "babel-plugin-styled-components": "^1.10.6",
    "eslint": "^6.3.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "yarn-run-all": "^3.1.1"
  }
}
